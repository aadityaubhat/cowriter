[flake8]
max-line-length = 100
extend-ignore = E203, W503, E501, B950
max-complexity = 10
exclude = .git,__pycache__,docs/source/conf.py,old,build,dist
ignore = B008
per-file-ignores =
    __init__.py:F401
docstring-convention = google
select = C,E,F,W,B
extend-immutable-calls = fastapi.Depends, fastapi.Query, fastapi.Path, fastapi.Body 